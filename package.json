    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/react-native": "^0.64.10",
    "@types/react-native-base64": "^0.1.0",
    "@types/react-native-global-props": "^1.1.0",
    "@types/react-native-snap-carousel": "^3.8.3",
